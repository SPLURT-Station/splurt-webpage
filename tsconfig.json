{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": [
      "bun-types",
      "vite-plugin-solid-svg/types",
      "unplugin-icons/types/astro",
      "unplugin-fonts/client"
    ]
  }
}
